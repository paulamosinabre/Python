"""1
valid, pass, fail"""

x = 0
z = 75
y = int(input("Enter grade: "))
print("Valid: ", x <= y <= 100)
print("Passed: ", y >= z and z >= 100)
print("Failed: ", y <= z and y >= 0)

"""2
odd even"""

a = 0

grade = int(input("Enter Grade: "))
print("Zero: ", grade == 0)
print("Even: ", a % 2 == 0 and grade != 0)
print("Odd: ", a % 2 == 1)
